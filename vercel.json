{
    "version": 2,
    "builds": [
      { "src": "src/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/public/(.*)", "dest": "/public/$1" },
      { "src": "/public/js/(.*)", "dest": "/public/js/$1" },
      { "src": "/public/css/(.*)", "dest": "/public/css/$1" },
      { "src": "/views/(.*)", "dest": "/views/$1" },
      { "src": "/(.*)", "dest": "/src/server.js" }
    ]
  }
  